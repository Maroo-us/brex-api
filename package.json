{
  "name": "@maroo-us/brex-api",
  "version": "0.2.0",
  "description": "Unofficial JS/TS wrapper for Brex (c) API.",
  "main": "dist/brex-api.js",
  "license": "MIT",
  "types": "dist/brex-api.d.ts",
  "keywords": [
    "brex",
    "brex bank",
    "brex api",
    "maroo",
    "maroo-us"
  ],
  "bugs": {
    "url": "https://github.com/Maroo-us/brex-api/issues",
    "email": "support@maroo.us"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Maroo-us/brex-api.git"
  },
  "contributors": [
    {
      "name": "Alexey Agachev",
      "email": "a.agachev@maroo.us"
    },
    {
      "name": "Andrei Riaskov",
      "email": "andrei@maroo.us"
    }
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "types": "api-extractor run",
    "dist": "yarn build && yarn types && npm pack",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "prepare": "husky install",
    "prepublishOnly": "yarn build && yarn types"
  },
  "dependencies": {
    "axios": "0.21.4",
    "snake-camel": "1.0.8"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.18.6",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "rollup": "2.53.0",
    "rollup-plugin-banner2": "1.2.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "typescript": "4.3.5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-console": "error",
      "eqeqeq": "error"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix"
    ],
    "**/*.{yaml,yml,json,md}": [
      "prettier --write"
    ]
  }
}
